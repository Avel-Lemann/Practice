<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="NewBoard" TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Button x:Name="NewBoard" HorizontalAlignment="Left" Height="58" Margin="-14,128,0,0" VerticalAlignment="Top" Width="228" FontSize="18" FontFamily="Comic Sans MS" Click="NewBoard_Click" Cursor="Hand">
                        <Button.Template>
                            <ControlTemplate TargetType="{x:Type Button}" >
                                <Border BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="0" CornerRadius="0,50,50,0" Margin="13,-16,-13,16">
                                    <Border.Background>White</Border.Background>
                                    <ContentPresenter Content="{TemplateBinding ContentControl.Content}" HorizontalAlignment="Center" VerticalAlignment="Center"></ContentPresenter>
                                </Border>
                            </ControlTemplate>
                        </Button.Template>
                        <Image Height="49" Source="Resources/Knopka1.png" Stretch="Fill" Width="171"/>
                    </Button>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
